{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "indent": [2, 2, {"SwitchCase": 1}],
        "eol-last": 0,
        "no-eval": 0,
        "max-len": 0,
        "no-underscore-dangle": 0,
        "prefer-template": 1,
        "no-unused-expressions": 1,
        "no-console": 0,
    },
    settings: {
      import/resolver: {
        webpack: {config: 'webpack/base.js'}
      }
    }
}
